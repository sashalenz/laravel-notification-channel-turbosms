{
    "name": "sashalenz/laravel-notification-channel-turbosms",
    "description": "Turbo SMS Notification channel for Laravel 6.0+.",
    "keywords": ["laravel", "notifications", "turbosms"],
    "homepage": "https://github.com/sashalenz/laravel-notification-channel-turbosms",
    "license": "MIT",
    "authors": [
        {
            "name": "yakimka",
            "email": "ss.yakim@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/yakimka"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/notifications": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\TurboSms\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\TurboSms\\TurboSmsServiceProvider"
            ]
        }
    }
}
